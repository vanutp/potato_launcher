{
    "download_server_base": "https://your.domain/<some path>",
    "resources_url_base": "https://your.domain/<some path>/assets/objects",
    "replace_download_urls": true,
    "version_manifest_url": "https://your.domain/<some path>/version_manifest.json",
    "versions": [
        {
            "name": "Monifactory",
            "minecraft_version": "1.20.1",
            "loader_name": "forge",
            "loader_version": "47.3.7",
            "include": [
                {
                    "path": "mods"
                },
                {
                    "path": "config",
                    "overwrite": false
                }
            ],
            "include_from": "<...>/PrismLauncher/instances/Monifactory/minecraft",
            "auth_backend": {
                "type": "telegram",
                "auth_base_url": "https://your.auth.domain"
            },
            "recommended_xmx": "8G"
        }
    ],
    "exec_after_all": "chmod -R +r ./generated && rsync -za --info=progress2 ./generated/ user@server:/path/to/your/dir/"
}